{
    "name": "jarzon/primbase",
    "type": "project",
    "autoload": {
        "psr-4": {"PrimBase\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"Tests\\": "app/tests/"}
    },
    "repositories": [
        {
            "type": "path",
            "url": "../Prim",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "jarzon/prim": "@dev",
        "robmorgan/phinx": "^0.6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "4.7.*",
        "phpunit/dbunit": ">=1.2"
    }
}